{
  "name": "typescript-redux-boilerplate",
  "version": "1.0.0",
  "description": "typescript-redux-boilerplate",
  "scripts": {
    "clean:server-dev": "rimraf src/static src/server.js src/server.js.map",
    "clean:server-prod": "rimraf build/server.js build/server.js.map",
    "clean:prod": "rimraf build",
    "clean": "npm run clean:server-dev && npm run clean:prod",
    "build:server-dev": "npm run clean:server-dev && APP_TARGET=server NODE_ENV=development node scripts/build-server",
    "build:server-prod": "npm run clean:server-prod && APP_TARGET=server NODE_ENV=production node scripts/build-server && rimraf build/static/bundle.css build/static/bundle.css.map",
    "build:client-prod": "npm run clean:prod && mkdir -p build && cp src/favicon.ico build/ && APP_TARGET=client NODE_ENV=production webpack --config webpack.config.js --progress",
    "build:prod": "npm run build:client-prod && npm run build:server-prod",
    "start:dev": "npm run build:server-dev && APP_TARGET=server NODE_ENV=development node src/server.js",
    "start:prod": "npm run build:prod && cd build && APP_TARGET=server NODE_ENV=production node server.js",
    "start": "npm run start:dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mezzario/typescript-redux-boilerplate.git"
  },
  "keywords": [
    "typescript",
    "react",
    "redux",
    "boilerplate"
  ],
  "author": "Eugene Baranchuk <vencedor@gmail.com> (http://github.com/mezzario)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mezzario/typescript-redux-boilerplate/issues"
  },
  "homepage": "https://github.com/mezzario/typescript-redux-boilerplate/",
  "dependencies": {
    "cheerio": "^0.20.0",
    "classnames": "^2.2.3",
    "core-js": "^2.1.3",
    "fastclick": "^1.0.6",
    "history": "^2.0.1",
    "lodash.debounce": "^4.0.3",
    "modernizr": "^3.3.1",
    "node-noop": "^1.0.0",
    "react": "^0.14.7",
    "react-document-title": "^2.0.1",
    "react-dom": "^0.14.7",
    "react-mixin": "^3.0.3",
    "react-motion": "^0.4.2",
    "react-pure-render": "^1.0.2",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.1",
    "redux-thunk": "^1.0.3",
    "semantic-ui": "^2.1.8",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.6.4",
    "babel-core": "^6.6.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.6.0",
    "chalk": "^1.1.1",
    "compression": "^1.6.1",
    "css-loader": "^0.23.1",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "image-webpack-loader": "^1.6.3",
    "json-loader": "^0.5.4",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "modernizr-loader": "^0.0.4",
    "ntypescript": "^1.201603040103.1",
    "postcss-loader": "^0.8.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.9.1"
  }
}
